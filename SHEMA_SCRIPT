CREATE  TABLE "REAL_EST_DATA".REAL_EST_SCH.DIM_DATE ( 
	DATE_ID              integer NOT NULL   ,
	DATE_ORIGINAL_FORMAT date    ,
	DATE_ISO_FORMAT      date    ,
	YEAR                 integer    ,
	QUARTER              integer    ,
	MONTH                integer    ,
	DAY                  integer    ,
	MONTHNAME            varchar(50)    ,
	DAYNAME              varchar(50)    ,
	WEEKOFTHEYEAR        integer    ,
	CONSTRAINT PK_DIM_DATE PRIMARY KEY ( DATE_ID )
 );

CREATE  TABLE "REAL_EST_DATA".REAL_EST_SCH.DIM_LOCATION ( 
	LOCATION_ID          integer NOT NULL   ,
	TOWN                 varchar(50)    ,
	ADDRESS              varchar(250)    ,
	CONSTRAINT PK_DIM_LOCATION PRIMARY KEY ( LOCATION_ID )
 );

CREATE  TABLE "REAL_EST_DATA".REAL_EST_SCH.DIM_PROPERTY_TYPE ( 
	PROPERTYTYPE_ID      integer NOT NULL   ,
	CONSTRAINT PK_DIM_PROPERTY_TYPE PRIMARY KEY ( PROPERTYTYPE_ID )
 );

CREATE  TABLE "REAL_EST_DATA".REAL_EST_SCH.DIM_RESIDENTIAL_TYPE ( 
	RESIDENTIALTYPE_ID   integer NOT NULL   ,
	CONSTRAINT PK_DIM_RESIDENTIAL_TYPE PRIMARY KEY ( RESIDENTIALTYPE_ID )
 );


CREATE  TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ( 
	FACT_ID              integer NOT NULL   ,
	SALES_AMOUNT         number(7,2)    ,
	ASSESSED_VALUE       number(7,2)    ,
	SALES_RATIO          number(6,4)    ,
	LOCATION_ID          integer    ,
	RECORDEDDATE_ID      integer    ,
	LISTEDDATE_ID        integer    ,
	PROPERTYTYPE_ID      integer    ,
	RESIDENTIALTYPE_ID   integer    ,
	CONSTRAINT PK_FACTS_SALES PRIMARY KEY ( FACT_ID )
 );

ALTER TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ADD CONSTRAINT FK_FACTS_REALESTATE_DIM_DATE FOREIGN KEY ( RECORDEDDATE_ID ) REFERENCES "REAL_EST_DATA".REAL_EST_SCH.DIM_DATE( DATE_ID ) ON DELETE NO ACTION ON UPDATE NO ACTION;

ALTER TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ADD CONSTRAINT FK_FACTS_REALESTATE_DIM_DATE_0 FOREIGN KEY ( LISTEDDATE_ID ) REFERENCES "REAL_EST_DATA".REAL_EST_SCH.DIM_DATE( DATE_ID ) ON DELETE NO ACTION ON UPDATE NO ACTION;

ALTER TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ADD CONSTRAINT FK_FACTS_REALESTATE_DIM_PROPERTY_TYPE FOREIGN KEY ( PROPERTYTYPE_ID ) REFERENCES "REAL_EST_DATA".REAL_EST_SCH.DIM_PROPERTY_TYPE( PROPERTYTYPE_ID ) ON DELETE NO ACTION ON UPDATE NO ACTION;

ALTER TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ADD CONSTRAINT FK_FACTS_REALESTATE_DIM_RESIDENTIAL_TYPE FOREIGN KEY ( RESIDENTIALTYPE_ID ) REFERENCES "REAL_EST_DATA".REAL_EST_SCH.DIM_RESIDENTIAL_TYPE( RESIDENTIALTYPE_ID ) ON DELETE NO ACTION ON UPDATE NO ACTION;

ALTER TABLE "REAL_EST_DATA".REAL_EST_SCH.FACTS_REALESTATE ADD CONSTRAINT FK_FACTS_REALESTATE_DIM_LOCATION FOREIGN KEY ( LOCATION_ID ) REFERENCES "REAL_EST_DATA".REAL_EST_SCH.DIM_LOCATION( LOCATION_ID ) ON DELETE NO ACTION ON UPDATE NO ACTION;